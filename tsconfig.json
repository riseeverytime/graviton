{
	"include": ["src/**/*"],
	"module": "commonjs",
	"noImplicitAny": false,
	"compilerOptions": {
		"sourceMap": true,
		"target": "es5",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"lib": ["es2019", "dom"],
		"resolveJsonModule": true
	},
	"root": ".",
	"exclude": ["node_modules", "plugins", "pluginsDist"]
}
